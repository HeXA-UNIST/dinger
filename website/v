BEGIN;
CREATE TABLE "board_board" (
    "id" integer NOT NULL PRIMARY KEY,
    "name" varchar(32) NOT NULL UNIQUE,
    "readable" integer NOT NULL,
    "writable" integer NOT NULL
)
;
CREATE TABLE "board_article" (
    "id" integer NOT NULL PRIMARY KEY,
    "subject" varchar(256) NOT NULL,
    "content" text NOT NULL,
    "private" bool NOT NULL,
    "hit" integer NOT NULL,
    "written" datetime NOT NULL,
    "board_id" integer NOT NULL REFERENCES "board_board" ("id"),
    "author_id" integer NOT NULL REFERENCES "auth_user" ("id")
)
;
CREATE TABLE "board_comment" (
    "id" integer NOT NULL PRIMARY KEY,
    "content" varchar(140) NOT NULL,
    "written" datetime NOT NULL,
    "article_id" integer NOT NULL REFERENCES "board_article" ("id"),
    "author_id" integer NOT NULL REFERENCES "auth_user" ("id")
)
;
CREATE TABLE "board_commentincomment" (
    "id" integer NOT NULL PRIMARY KEY
)
;
CREATE TABLE "board_likes" (
    "id" integer NOT NULL PRIMARY KEY,
    "when" datetime NOT NULL,
    "who_id" integer NOT NULL REFERENCES "auth_user" ("id"),
    "what_id" integer NOT NULL REFERENCES "board_article" ("id")
)
;
CREATE INDEX "board_article_7d7b3da9" ON "board_article" ("board_id");
CREATE INDEX "board_article_e969df21" ON "board_article" ("author_id");
CREATE INDEX "board_comment_e669cc35" ON "board_comment" ("article_id");
CREATE INDEX "board_comment_e969df21" ON "board_comment" ("author_id");
CREATE INDEX "board_likes_ca305b63" ON "board_likes" ("who_id");
CREATE INDEX "board_likes_f6bb3872" ON "board_likes" ("what_id");

COMMIT;
BEGIN;
CREATE TABLE "member_photo" (
    "id" integer NOT NULL PRIMARY KEY
)
;
CREATE TABLE "member_userprofile" (
    "id" integer NOT NULL PRIMARY KEY,
    "name" varchar(32) NOT NULL,
    "birthday" date NOT NULL,
    "intro" text NOT NULL,
    "phone" varchar(12) NOT NULL,
    "user_id" integer NOT NULL UNIQUE REFERENCES "auth_user" ("id")
)
;

COMMIT;
